diff --git a/models/providers.js b/models/providers.js
new file mode 100644
index 0000000..22d84ca
--- /dev/null
+++ b/models/providers.js
@@ -0,0 +1,32 @@
+providers = new Mongo.Collection('providers');
+
+providers.attachSchema(
+    new SimpleSchema({
+    title: {
+      type: String
+    },
+    content: {
+      type: String
+    },
+    createdAt: {
+      type: Date,
+      denyUpdate: true
+    }
+  })
+);
+
+// Collection2 already does schema checking
+// Add custom permission rules if needed
+if (Meteor.isServer) {
+  providers.allow({
+    insert : function () {
+      return true;
+    },
+    update : function () {
+      return true;
+    },
+    remove : function () {
+      return true;
+    }
+  });
+}
