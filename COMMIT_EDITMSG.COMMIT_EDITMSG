+servicelist = new Mongo.Collection('servicelist');
+
+servicelist.attachSchema(
+    new SimpleSchema({
+    title: {
+      type: String
+    },
+    content: {
+      type: String
+    },
+    createdAt: {
+      type: Date,
+      denyUpdate: true
+    }
+  })
+);
+
+// Collection2 already does schema checking
+// Add custom permission rules if needed
+if (Meteor.isServer) {
+  servicelist.allow({
+    insert : function () {
+      return true;
+    },
+    update : function () {
+      return true;
+    },
+    remove : function () {
+      return true;
+    }
+  });
+}
