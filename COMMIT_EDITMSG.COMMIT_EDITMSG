+function loadFixture(fixtures, collection) {
+  var i;
+
+  for (i = 0; i < fixtures.length; i+= 1) {
+    //collection.remove({ });
+    collection.insert(fixtures[i]);
+  }
+}
+
+Meteor.startup(function () {
+  //loadFixture(Fixtures['dummyFixture'], DummyCollection);
+});
diff --git a/server/startup/loadUsers.js b/server/startup/loadUsers.js
new file mode 100644
index 0000000..8b3f82b
--- /dev/null
+++ b/server/startup/loadUsers.js
@@ -0,0 +1,15 @@
+function loadUser(user) {
+  var userAlreadyExists = typeof Meteor.users.findOne({ username : user.username }) === 'object';
+
+  if (!userAlreadyExists) {
+    Accounts.createUser(user);
+  }
+}
+
+Meteor.startup(function () {
+  var users = YAML.eval(Assets.getText('users.yml'));
+
+  for (key in users) if (users.hasOwnProperty(key)) {
+    loadUser(users[key]);
+  }
+});
\ No newline at end of file
