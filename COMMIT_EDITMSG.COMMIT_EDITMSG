+.ui.small.step,
+.ui.small.steps .step {
+  font-size: @small;
+}
+.ui.step,
+.ui.steps .step {
+  font-size: @medium;
+}
+.ui.large.step,
+.ui.large.steps .step {
+  font-size: @large;
