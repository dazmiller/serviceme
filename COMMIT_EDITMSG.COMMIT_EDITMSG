diff --git a/client/lib/semantic-ui/theme.import.less b/client/lib/semantic-ui/theme.import.less
new file mode 100644
index 0000000..d6e3659
--- /dev/null
+++ b/client/lib/semantic-ui/theme.import.less
@@ -0,0 +1,52 @@
+/*
+  DO NOT MODIFY - This file has been generated and will be regenerated
+  Semantic UI v1.12.1
+*/
+/*******************************
+        Import Directives
+*******************************/
+
+@theme : @@element;
+
+/*------------------
+    Default Theme
+-------------------*/
+
+@import "@{themesFolder}/default/globals/site.variables";
+@import "@{themesFolder}/default/@{type}s/@{element}.variables";
+
+/*------------------
+   Packaged Theme
+-------------------*/
+
+@import "@{themesFolder}/@{site}/globals/site.variables";
+@import "@{themesFolder}/@{theme}/@{type}s/@{element}.variables";
+
+/*------------------
+     Site Theme
+-------------------*/
+
+@import "@{siteFolder}/globals/site.variables.import.less";
+@import "@{siteFolder}/@{type}s/@{element}.variables.import.less";
+
+
+/*******************************
+        Import Mix-ins
+*******************************/
+
+/*------------------
+       Fonts
+-------------------*/
+
+.loadFonts() when (@importGoogleFonts) {
+  @import (css) '@{googleProtocol}fonts.googleapis.com/css?family=@{googleFontRequest}';
+}
+
+/*------------------
+     Overrides
+-------------------*/
+
+.loadUIOverrides() {
+  @import "@{themesFolder}/@{theme}/@{type}s/@{element}.overrides";
+  @import "@{siteFolder}/@{type}s/@{element}.overrides.import.less";
+}
