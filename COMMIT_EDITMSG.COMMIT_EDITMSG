+---------------*/
+
+/* Initial */
+.ui.slide.along.sidebar {
+  z-index: @bottomLayer;
+}
+
+/* Sidebar - Initial */
+.ui.left.slide.along.sidebar {
+  transform: translate3d(-50%, 0, 0);
+}
+.ui.right.slide.along.sidebar {
