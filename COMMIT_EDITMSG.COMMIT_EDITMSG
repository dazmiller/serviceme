+  background: @checkboxSelectedBackground;
+  border: 1px solid @checkboxSelectedBorder;
+}
+.ui.checkbox input[type="checkbox"]:focus ~ label,
+.ui.checkbox input[type="radio"]:focus ~ label {
+  color: @labelSelectedColor;
+}
